import java.util.*;
public class NeedsCleaning {
private State roomState;
private String cleanTime;
Scanner scan = new Scanner(System.in);
public void getState() {
	return roomState;
}
public void timeToClean() {
	System.out.println("Enter scheduled time to clean: ");
	cleanTime = scan.next();
	}
public void checkIn() {
	System.out.println("Error: Cannot check in, this room must be cleaned first.");
	return;
}
public void checkOut() {
	System.out.println("Error: Cannot check out, this room is unoccupied.");
	return;
}

}
